{
  "name": "@amoutonbrady/tiny-http",
  "version": "0.6.0",
  "description": "A tiny client side HTTP client, light and extensible",
  "sideEffects": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/amoutonbrady/tiny-http.git"
  },
  "main": "dist/http.common.cjs",
  "module": "dist/http.module.js",
  "browser": "dist/http.module.js",
  "unpkg": "dist/http.min.js",
  "exports": {
    ".": {
      "require": "./dist/http.common.cjs",
      "import": "./dist/http.module.js"
    }
  },
  "types": "dist/http.d.ts",
  "scripts": {
    "build": "rm -rf dist && rollup -c && tsc",
    "prepublishOnly": "yarn build && yarn test",
    "test": "uvu -r jiti/register tests",
    "changelog": "npx gitmoji-changelog",
    "check": "npx @skypack/package-check",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "http",
    "light",
    "typescript",
    "dependencies free"
  ],
  "author": "Alexandre Mouton-Brady <amoutonbrady@gmail.com>",
  "license": "MIT",
  "// No dependencies hehe": "",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@types/node": "^14.14.20",
    "@types/node-fetch": "^2.5.7",
    "esbuild": "^0.11.15",
    "jiti": "^1.9.1",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "rollup": "^2.36.1",
    "rollup-plugin-esbuild": "^4.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.3.0-dev.20210427",
    "uvu": "^0.5.1"
  }
}
